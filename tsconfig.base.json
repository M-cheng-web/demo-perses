{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "compilerOptions": {
    "baseUrl": ".",
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "jsx": "preserve",
    "strict": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "useDefineForClassFields": true,
    "types": ["vite/client"],
    "paths": {
      "/#/*": ["packages/dashboard/src/*"],
      "@grafana-fast/dashboard": ["packages/dashboard/src/index.ts"],
      "@grafana-fast/component": ["packages/component/src/index.ts"],
      "@grafana-fast/hooks": ["packages/hook/src/index.ts"],
      "@grafana-fast/api": ["packages/api/src/index.ts"],
      "@grafana-fast/panels": ["packages/panels/src/index.ts"],
      "@grafana-fast/store": ["packages/store/src/index.ts"],
      "@grafana-fast/types": ["packages/types/src/index.ts"],
      "@grafana-fast/types/*": ["packages/types/src/*"]
    }
  }
}
