{
  "name": "@grafana-fast/types",
  "version": "0.1.0",
  "description": "Shared type definitions for the grafana-fast monorepo.",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./*": "./dist/*"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -b --clean && tsc -b -f",
    "type-check": "tsc --noEmit -p tsconfig.json"
  },
  "peerDependencies": {
    "vue": "^3.5.0"
  },
  "license": "MIT"
}
