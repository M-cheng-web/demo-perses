{
  "name": "@grafana-fast/api",
  "version": "0.1.0",
  "description": "Data access layer (contracts + implementations) for grafana-fast.",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./mock": {
      "types": "./dist/mock.d.ts",
      "import": "./dist/mock.mjs",
      "require": "./dist/mock.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c ../../scripts/rollup.config.js",
    "type-check": "tsc --noEmit -p tsconfig.json"
  },
  "dependencies": {
    "@grafana-fast/types": "workspace:*",
    "@grafana-fast/utils": "workspace:*"
  },
  "license": "MIT"
}
