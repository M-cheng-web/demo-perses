{
  "name": "@grafana-fast/hooks",
  "version": "0.1.0",
  "description": "Composable hooks for interacting with grafana-fast dashboards.",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c ../../scripts/rollup.config.js"
  },
  "dependencies": {
    "@grafana-fast/api": "workspace:*",
    "@grafana-fast/store": "workspace:*",
    "@grafana-fast/dashboard": "workspace:*",
    "@grafana-fast/utils": "workspace:*",
    "@grafana-fast/types": "workspace:*"
  },
  "peerDependencies": {
    "echarts": "^6.0.0",
    "vue": "^3.5.0"
  },
  "license": "MIT"
}
