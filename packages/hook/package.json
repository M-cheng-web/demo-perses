{
  "name": "@grafana-fast/hooks",
  "version": "0.1.0",
  "description": "Composable hooks for interacting with grafana-fast dashboards.",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c ../../scripts/rollup.config.js",
    "type-check": "tsc --noEmit -p tsconfig.json",
    "type-check:dev": "pnpm -C ../dashboard run build:types && tsc --noEmit -p tsconfig.json",
    "test": "pnpm run test:api-mode && pnpm run test:mount-lifecycle",
    "test:api-mode": "esno scripts/use-dashboard-api-mode.test.ts",
    "test:mount-lifecycle": "esno scripts/use-dashboard-mount-lifecycle.test.ts"
  },
  "dependencies": {
    "@grafana-fast/api": "workspace:*",
    "@grafana-fast/store": "workspace:*",
    "@grafana-fast/dashboard": "workspace:*",
    "@grafana-fast/utils": "workspace:*",
    "@grafana-fast/types": "workspace:*"
  },
  "peerDependencies": {
    "echarts": "^6.0.0",
    "vue": "^3.5.0"
  },
  "license": "MIT"
}
