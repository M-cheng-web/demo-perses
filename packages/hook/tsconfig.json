{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "/#/*": ["../dashboard/src/*"],
      "@grafana-fast/dashboard": ["../dashboard/src/index.ts"],
      "@grafana-fast/component": ["../component/src/index.ts"],
      "@grafana-fast/store": ["../store/src/index.ts"],
      "@grafana-fast/types": ["../types/src/index.ts"],
      "@grafana-fast/types/*": ["../types/src/*"]
    },
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "outDir": "dist",
    "rootDir": "src",
    "module": "ESNext",
    "target": "ESNext",
    "noEmit": false
  },
  "include": ["src/**/*.ts"],
  "exclude": ["dist"],
  "references": [{ "path": "../types" }, { "path": "../store" }, { "path": "../dashboard" }]
}
