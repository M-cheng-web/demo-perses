{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "baseUrl": "./",
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.tsbuildinfo",
    "paths": {
      "/#/*": ["../dashboard/dist/src/*"],
      "@grafana-fast/dashboard": ["../dashboard/dist/src/index.d.ts"],
      "@grafana-fast/store": ["../store/dist/index.d.ts"],
      "@grafana-fast/utils": ["../utils/dist/index.d.ts"],
      "@grafana-fast/types": ["../types/dist/index.d.ts"],
      "@grafana-fast/types/*": ["../types/dist/*"]
    },
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "allowImportingTsExtensions": false,
    "outDir": "dist",
    "rootDir": "src",
    "module": "ESNext",
    "target": "ESNext",
    "noEmit": false
  },
  "include": ["src/**/*.ts", "src/**/*.d.ts"],
  "exclude": ["dist"],
  "references": [{ "path": "../types" }, { "path": "../store" }]
}
