{
  "name": "@grafana-fast/dashboard",
  "version": "0.1.0",
  "description": "Dashboard experience package for grafana-fast with custom UI kit.",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "vite build --config vite.config.ts",
    "build:types": "vue-tsc --project tsconfig.build-types.json --declaration --emitDeclarationOnly --outDir dist && esno ../../scripts/rewrite-dashboard-dts-aliases.ts",
    "type-check": "vue-tsc --noEmit --project tsconfig.json",
    "preview": "vite preview"
  },
  "dependencies": {
    "@grafana-fast/api": "workspace:*",
    "@grafana-fast/store": "workspace:*",
    "@grafana-fast/component": "workspace:*",
    "@grafana-fast/json-editor": "workspace:*",
    "@grafana-fast/promql": "workspace:*",
    "@grafana-fast/types": "workspace:*",
    "@grafana-fast/utils": "workspace:*",
    "@interactjs/core": "1.10.27",
    "@interactjs/utils": "1.10.27",
    "vue-grid-layout-v3": "^3.1.2"
  },
  "peerDependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "echarts": "^6.0.0",
    "vue": "^3.5.0"
  },
  "devDependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "echarts": "^6.0.0",
    "less": "^4.4.2",
    "sass": "^1.94.0",
    "typescript": "~5.9.3",
    "vite": "^7.2.2",
    "vue": "^3.5.24",
    "vue-tsc": "^3.1.3"
  },
  "sideEffects": [
    "**/*.css",
    "**/*.less",
    "**/*.scss"
  ],
  "license": "MIT"
}
