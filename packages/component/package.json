{
  "name": "@grafana-fast/component",
  "version": "0.1.0",
  "description": "Lightweight tech-styled UI kit for grafana-fast dashboards.",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./styles": {
      "types": "./dist/styles.d.ts",
      "import": "./dist/styles.mjs",
      "require": "./dist/styles.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "vite build --config vite.config.ts",
    "build:types": "node -e \"require('fs').rmSync('node_modules/.tmp/tsconfig.tsbuildinfo', { force: true })\" && vue-tsc --project tsconfig.json --declaration --emitDeclarationOnly --outDir dist",
    "type-check": "vue-tsc --noEmit --project tsconfig.json",
    "preview": "vite preview"
  },
  "dependencies": {
    "@grafana-fast/utils": "workspace:*"
  },
  "peerDependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "vue": "^3.5.0"
  },
  "devDependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "less": "^4.4.2",
    "typescript": "~5.9.3",
    "vite": "^7.2.2",
    "vue": "^3.5.24",
    "vue-tsc": "^3.1.3"
  },
  "sideEffects": [
    "**/*.css",
    "**/*.less"
  ],
  "license": "MIT"
}
