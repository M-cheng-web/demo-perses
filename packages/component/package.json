{
  "name": "@grafana-fast/component",
  "version": "0.1.0",
  "description": "Dashboard UI components for grafana-fast.",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "vite build --config vite.config.ts",
    "build:types": "vue-tsc --project tsconfig.json --declaration --emitDeclarationOnly --outDir dist",
    "preview": "vite preview"
  },
  "dependencies": {
    "@grafana-fast/store": "workspace:*",
    "@grafana-fast/types": "workspace:*",
    "axios": "^1.13.2",
    "lodash-es": "^4.17.21",
    "monaco-editor": "^0.54.0",
    "uuid": "^13.0.0",
    "vue-grid-layout-v3": "^3.1.2"
  },
  "peerDependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "ant-design-vue": "^4.0.0",
    "dayjs": "^1.11.0",
    "echarts": "^6.0.0",
    "vue": "^3.5.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.1",
    "less": "^4.4.2",
    "sass": "^1.94.0",
    "typescript": "~5.9.3",
    "vite": "^7.2.2",
    "vue-tsc": "^3.1.3"
  },
  "sideEffects": [
    "**/*.css",
    "**/*.less",
    "**/*.scss"
  ],
  "license": "MIT"
}
