{
  "name": "@grafana-fast/utils",
  "version": "0.1.0",
  "description": "Shared utilities (bem/time/http/common) for grafana-fast packages.",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c ../../scripts/rollup.config.js",
    "type-check": "tsc --noEmit -p tsconfig.json",
    "test": "pnpm run test:promql && pnpm run test:promql-parse",
    "test:promql": "esno scripts/promql-render.test.ts",
    "test:promql-parse": "esno scripts/promql-parse.test.ts"
  },
  "dependencies": {
    "@grafana-fast/types": "workspace:*"
  },
  "license": "MIT"
}
