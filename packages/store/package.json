{
  "name": "@grafana-fast/store",
  "version": "0.1.0",
  "description": "Lightweight Pinia-like store for grafana-fast without external dependency.",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c ../../scripts/rollup.config.js",
    "build:types": "tsc -b -f --emitDeclarationOnly",
    "type-check": "tsc --noEmit -p tsconfig.json"
  },
  "peerDependencies": {
    "vue": "^3.5.0"
  },
  "license": "MIT"
}
