{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@grafana-fast/store": ["./src/index.ts"]
    },
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.tsbuildinfo",
    "declaration": true,
    "declarationMap": true,
    "allowImportingTsExtensions": false,
    "outDir": "dist",
    "rootDir": "src",
    "module": "ESNext",
    "target": "ESNext",
    "noEmit": false
  },
  "include": ["src/**/*.ts"],
  "exclude": ["dist"]
}
