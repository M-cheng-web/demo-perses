{
  "name": "@grafana-fast/monorepo",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@7.6.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "preinstall": "node ./scripts/ensure-pnpm.mjs",
    "dev": "pnpm -C packages/app dev",
    "dev:dist": "GF_USE_DIST=1 pnpm -C packages/app dev",
    "build": "pnpm run build:packages && pnpm -C packages/app build",
    "build:dist": "pnpm run build:packages && GF_USE_DIST=1 pnpm -C packages/app build",
    "preview": "pnpm -C packages/app preview",
    "type-check": "pnpm -C packages/app type-check",
    "build:packages": "pnpm run build:types && pnpm run build:store && pnpm run build:component && pnpm run build:dashboard && pnpm run build:hooks",
    "build:component": "pnpm -C packages/component run build && pnpm -C packages/component run build:types",
    "build:dashboard": "pnpm -C packages/dashboard run build && pnpm -C packages/dashboard run build:types",
    "build:hooks": "pnpm -C packages/hook run build",
    "build:store": "pnpm -C packages/store run build",
    "build:types": "pnpm -C packages/types run build",
    "docs:dev": "pnpm -C packages/docs run docs:dev",
    "docs:build": "pnpm -C packages/docs run docs:build",
    "lint": "eslint . --ext .ts,.tsx,.vue",
    "format": "prettier --check .",
    "scripts:build": "ts-node --esm --project tsconfig.scripts.json scripts/build.ts"
  },
  "pnpm": {
    "overrides": {
      "esbuild": "0.25.12"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.10.0",
    "@types/uuid": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "@rollup/plugin-alias": "^5.1.0",
    "consola": "^2.15.3",
    "esbuild": "0.25.12",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-define-config": "1.23.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "fast-glob": "^3.2.12",
    "fs-extra": "^10.1.0",
    "prettier": "^3.0.2",
    "rollup": "^4.24.0",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-esbuild": "^6.1.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.3",
    "vite": "^7.2.2",
    "vue": "^3.5.24",
    "vue-tsc": "^3.1.3"
  },
  "engines": {
    "node": ">=18"
  }
}
